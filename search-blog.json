---
layout: null
---
[
  {% for item in site.posts %}
    {
      "title"    : "{{ item.title | escape }}",
      "category" : "{{ item.category }}",
      "tags"     : "{{ item.tags | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ item.url }}",
      "date"     : "{{ item.date }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]